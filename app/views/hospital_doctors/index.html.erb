<div> <h1> Doctors </h1>
<%= link_to "Add Doctor", "/hospitals/#{@hospital.id}/doctors/new" %>
<% @doctors.each do |doctor| %>
    <h3><%= link_to doctor.name, "/doctors/#{doctor.id}"%> </h3>
    <p>Specialty: <%= doctor.specialty %></p>
    <p>License: <%= doctor.license %></p>
    <p>Offers Probono: <%= doctor.probono %></p>
    

<% end %>
</div,>