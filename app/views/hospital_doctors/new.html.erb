<%= form_with url: "/hospitals/#{@hospital_id}/doctors", method: :post, local: true do |form|%>

<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :specialty %>
<%= form.text_field :specialty %>
<%= form.label :license %>
<%= form.text_field :license %>
<%= form.label :probono %>
<%= form.text_field :probono %>

<%= form.submit "Add Doctor" %>

<% end %>